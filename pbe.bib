  issn = {0010-2180},
  keywords = {Soot formation, Planar laser-induced fluorescence (PLIF), Elastic
	light scattering (ELS), Line-of-sight attenuation (LOSA), Population
	balance, Sectional method, Laminar flame},
  owner = {fsewerin},
  timestamp = {2020.08.24},
  url = {http://www.sciencedirect.com/science/article/pii/S0010218020302546}
}

@Article{Liu2019b,
  author    = {Liu, Anxiong and Rigopoulos, Stelios},
