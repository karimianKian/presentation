@Article{Sujith2016,
  author    = {Sujith, RI and Juniper, MP and Schmid, PJ},
  journal   = {International Journal of Spray and Combustion Dynamics},
  title     = {Non-normality and nonlinearity in thermoacoustic instabilities},
