  groups    = {fsewerin:1, Metal combustion},
  keywords  = {Metal combustion, Combustion instabilities, Bluff-body stabilized flames, NO emissions, Clean energy sources},
  owner     = {fsewerin},
  timestamp = {2019.03.19},
  url       = {http://www.sciencedirect.com/science/article/pii/S154074891830436X},
}

@ARTICLE{Lasheras2002,
