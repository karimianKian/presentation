@Article{Mikhaylov2021,
  author    = {Mikhaylov, Kirill and Rigopoulos, Stelios and Papadakis, George},
  journal   = {AIChE Journal},
  title     = {Reconstruction of large-scale flow structures in a stirred tank from limited sensor data},
  year      = {2021},
  issn      = {0001-1541},
