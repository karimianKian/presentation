@ARTICLE{Eaves2016,
  author = {Eaves, Nick A. and Zhang, Qingan and Liu, Fengshan and Guo, Hongsheng
	and Dworkin, Seth B. and Thomson, Murray J.},
  title = {CoFlame: A refined and validated numerical algorithm for modeling
	sooting laminar coflow diffusion flames},
  journal = {Computer Physics Communications},
  year = {2016},
  volume = {207},
  pages = {464--477},
  abstract = {Mitigation of soot emissions from combustion devices is a global concern.
	For example, recent EURO 6 regulations for vehicles have placed stringent
	limits on soot emissions. In order to allow design engineers to achieve
	the goal of reduced soot emissions, they must have the tools to so.
	Due to the complex nature of soot formation, which includes growth
	and oxidation, detailed numerical models are required to gain fundamental
	insights into the mechanisms of soot formation. A detailed description
